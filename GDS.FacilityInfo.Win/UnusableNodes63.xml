  <NavigationItems Id="NavigationItems">
    <Items Id="Items">
      <Item Id="navitemvertragsVerwaltung">
        <Items Id="Items">
          <Item Id="boVertragsleistung_ListView" ViewId="boVertragsleistung_ListView" />
