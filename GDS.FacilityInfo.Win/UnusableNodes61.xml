                    <TabbedGroup Id="tabAdrMain">
                      <LayoutGroup Id="grpKontakte">
                        <LayoutItem Id="viewItemKontakteLiegenschaft" ViewItem="KontakteLiegenschaft" />
                      </LayoutGroup>
                    </TabbedGroup>
                  </LayoutGroup>
                </TabbedGroup>
              </LayoutGroup>
            </LayoutGroup>
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
