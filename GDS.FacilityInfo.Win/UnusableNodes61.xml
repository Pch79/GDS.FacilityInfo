  <NavigationItems Id="NavigationItems">
    <Items Id="Items">
      <Item Id="navitemvertragsVerwaltung">
        <Items Id="Items">
          <Item Id="boWartungsVertrag_ListView" ViewId="boWartungsVertrag_ListView" />
