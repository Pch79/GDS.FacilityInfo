    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="EasyTestConnectionString" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Integrated Security=SSPI;Pooling=false;Data Source=(localdb)\mssqllocaldb;Initial Catalog=GDS.FacilityInfoEasyTest" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\EasyTestConnectionString_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
